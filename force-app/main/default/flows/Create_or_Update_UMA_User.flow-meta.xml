    <formulas>
        <name>Email</name>
        <dataType>String</dataType>
        <expression>IF({!Organization.IsSandbox}, 
{!UserEntry.Email__c} &amp; &apos;.invalid&apos;,
{!UserEntry.Email__c}
)</expression>
    </formulas>
