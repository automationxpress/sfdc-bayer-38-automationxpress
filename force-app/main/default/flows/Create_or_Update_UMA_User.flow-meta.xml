    </formulas>
    <formulas>
        <name>Username</name>
        <dataType>String</dataType>
        <expression>IF({!Organization.IsSandbox}, 
{!UserEntry.Email__c} &amp; &apos;.test&apos;,
{!UserEntry.Email__c} &amp; &apos;.prod&apos;
)</expression>
